// Class to implement operator overloading\par
// function for concatenating the strings\par
class CompareString \{\par
 \par
public:\par
    // Classes object of string\par
    char str[25];\par
 \par
    // Parameterized Constructor\par
    CompareString(char str1[])\par
    \{\par
        // Initialize the string to class object\par
        strcpy(this->str, str1);\par
    \}\par
 \par
    // Overloading '==' under a function\par
    // which returns integer 1/true\par
    // if left operand string\par
    // and right operand string are equal.\par
    //(else return 0/false)\par
    int operator==(CompareString s2)\par
    \{\par
        if (strcmp(str, s2.str) == 0)\par
            return 1;\par
        else\par
            return 0;\par
    \}\par
// Overloading '<=' under a function\par
    // which returns integer 1/true\par
    // if left operand string is smaller than\par
    // or equal to the right operand string.\par
    // (else return 0/false)\par
    int operator<=(CompareString s3)\par
    \{\par
        if (strlen(str) <= strlen(s3.str))\par
            return 1;\par
        else\par
            return 0;\par
    \}\par
 \par
    // Overloading '>=' under a function\par
    // which returns integer 1/true\par
    // if left operand string is larger than\par
    // or equal to the right operand string.\par
    //(else return 0/false)\par
    int operator>=(CompareString s3)\par
    \{\par
        if (strlen(str) >= strlen(s3.str))\par
            return 1;\par
        else\par
            return 0;\par
    \}\par
\};\par
 \par
\par
\par
\par
\par
\par
