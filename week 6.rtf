class func\par
\{\par
        public:\par
                void pal(int );\par
                void pal(char c[]);\par
\};\par
void func :: pal(int n)\par
\{\par
        int n1,sum,a;\par
        n1=n;\par
        sum=0;\par
\par
        do\par
        \{\par
                sum=sum*10;\par
                a=n%10;\par
                sum=sum+a;\par
                n=n/10;\par
        \}while(n>0);\par
        if(sum==n1)\par
                cout<<"\\n it is Palindrome";\par
        else\par
                cout<<"\\n it is not a palindrome";\par
\par
\}\par
void func :: pal(char c[])\par
\{\par
        int l=strlen(c);\par
char b[10];\par
        int i,j;\par
        for(i=0,j=l-1;i<l;i++,j--)\par
        \{\par
                b[i]=c[j];\par
        \}\par
        b[i]=' ';\par
        if(strcmp(c,b)==0)\par
        \{cout<<"\\nThe entered text is palindrome";\par
        \}\par
        else\par
        \{cout<<"\\n the entered text is not a palindrome";\par
        \}\par
\}\par
int main()\par
\{\par
        char c[10];\par
        int k,r,v;\par
        func p;\par
        do\par
        \{\par
                cout<<"palindrome"<<endl;\par
                cout<<"1.for interger"<<endl;\par
                cout<<"2.for character"<<endl;\par
